<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="ReadFromKVM" mapIdentifier="llm-plugin-domain-mapping">
    <ExpiryTimeInSecs>10</ExpiryTimeInSecs>
    <Get assignTo="chatgt_plugin_name">
        <Key>
            <Parameter ref="request.header.host"/>
        </Key>
    </Get>
    <Get assignTo="chatgpt_plugin_bucket">
        <Key>
            <Parameter>chatgpt-plugin-bucket</Parameter>
        </Key>
    </Get>
    <Scope>organization</Scope>
</KeyValueMapOperations>