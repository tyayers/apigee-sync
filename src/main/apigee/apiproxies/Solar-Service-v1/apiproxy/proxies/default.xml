<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
  <Description/>
  <FaultRules/>
  <PreFlow name="PreFlow">
    <Request>
      <Step>
        <Name>VA-VerifyKey</Name>
      </Step>
      <Step>
        <Name>AM-RemoveKey</Name>
      </Step>
      <Step>
        <Name>EV-GetInputs</Name>
      </Step>
      <Step>
        <Condition>(apigee.latitude == null OR apigee.longitude == null) OR (apigee.address == null)</Condition>
        <Name>RF-ValidateInputs</Name>
      </Step>
      <Step>
        <Condition>apigee.address != null</Condition>
        <Name>SC-GetGeolocation</Name>
      </Step>
    </Request>
    <Response/>
  </PreFlow>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <Flows/>
  <HTTPProxyConnection>
    <BasePath>/v1/solar-service</BasePath>
    <Properties/>
    <VirtualHost>default</VirtualHost>
  </HTTPProxyConnection>
  <RouteRule name="default">
    <TargetEndpoint>default</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>
